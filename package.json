{
  "name": "@myupmc/typescript-package-template",
  "version": "0.0.0",
  "description": "A template repo for publishing typescript packages to npm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "ts-mocha -p tsconfig.json tests/**/*.test.ts",
    "test:ci": "nyc ts-mocha -p tsconfig.json tests/**/*.test.ts",
    "test:watch": "ts-mocha -p tsconfig.json tests/**/*.test.ts --watch",
    "clean:dist": "node -e \"require('del').sync(['dist/**'])\"",
    "prebuild": "npm run clean:dist",
    "build": "tsc --listFiles",
    "watch": "tsc -w",
    "start": "npm link && nodemon",
    "deploy": "npm-deploy-git-tag"
  },
  "repository": {
    "type": "git",
    "url": "https://git.tdc.upmc.edu/myupmc/npm/typescript-package-template"
  },
  "author": {
    "name": "myupmc",
    "email": "myupmc_svc@upmc.edu"
  },
  "license": "ISC",
  "homepage": "https://git.tdc.upmc.edu/myupmc/npm/typescript-package-template",
  "devDependencies": {
    "@hutson/npm-deploy-git-tag": "^6.0.0",
    "@types/chai": "^4.2.13",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "del": "^6.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.1.0",
    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
